	.popup.popup-auth.mfp-hide#popup-auth
		button.popup-close(data-mfp-close)
			+icon('ico-close')
		form(action="#")
			.popup-logo
				+icon('ico-logo')
			input(type="email", placeholder="Электронная почта").ui-input
			input(type="password", placeholder="Пароль").ui-input
			.row
				.col
					label.ui-check
						input(type="checkbox").ui-check__input
						span.ui-check__checkbox
							+icon('ico-check')
